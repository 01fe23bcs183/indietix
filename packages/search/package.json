{
  "name": "@indietix/search",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./parser": {
      "import": "./dist/parser.js",
      "types": "./dist/parser.d.ts"
    },
    "./rank": {
      "import": "./dist/rank.js",
      "types": "./dist/rank.d.ts"
    },
    "./embeddings": {
      "import": "./dist/embeddings.js",
      "types": "./dist/embeddings.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@xenova/transformers": "^2.17.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.9.3",
    "vitest": "^1.6.0"
  }
}
